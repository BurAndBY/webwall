{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "lib": ["DOM", "ES2019"],
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "noEmitOnError": true
  },
  "include": ["src/**/*"]
}
